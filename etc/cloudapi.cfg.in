{
    "port": 443,
    "certificate": "/opt/smartdc/cloudapi/ssl/cert.pem",
    "key": "/opt/smartdc/cloudapi/ssl/key.pem",
    "read_only": false,
    "create_images": false,
    "ufds": {
        "url": "ldaps://127.0.0.1",
        "bindDN": "UFDS_ROOT_DN",
        "bindPassword": "UFDS_ROOT_PW",
        "cache": {
            "size": 5000,
            "expiry": 60
        },
        "clientTimeout": 3000
    },
    "wfapi": {
        "url": "WFAPI_URL",
        "cache": {
            "size": 1000,
            "expiry": 300
        }
    },
    "vmapi": {
        "url": "VMAPI_URL",
        "cache": {
            "size": 5000,
            "expiry": 60
        }
    },
    "cnapi": {
        "url": "CNAPI_URL",
        "cache": {
            "size": 5000,
            "expiry": 60
        }
    },
    "napi": {
        "url": "NAPI_URL",
        "cache": {
            "size": 5000,
            "expiry": 300
        }
    },
    "fwapi": {
        "url": "FWAPI_URL",
        "cache": {
            "size": 1000,
            "expiry": 300
        }
    },
    "imgapi": {
        "url": "IMGAPI_URL",
        "cache": {
            "size": 5000,
            "expiry": 300
        }
    },
    "ca": {
        "url": "CA_URL"
    },
    "datacenters": {
        "DATACENTER_NAME": "CLOUDAPI_EXTERNAL_URL"
    },
    "ufds_admin_uuid": "UFDS_ADMIN_UUID",
    "plugins": [
        {
            "name": "provision_limits",
            "enabled": false,
            "config": {
                "datacenter": "DATACENTER_NAME",
                "defaults":[{
                   "os": "any",
                   "dataset": "any",
                   "check": "os",
                   "by": "ram",
                   "value": 51200
                }, {
                   "os": "windows",
                   "dataset": "windows",
                   "check": "dataset",
                   "by": "machines",
                   "value": -1
                }]
            }
        },
        {
            "name": "capi_limits",
            "enabled": false,
            "config": {
                "datacenter": "DATACENTER_NAME",
                "defaults": {
                    "smartos": 1,
                    "nodejs": 1,
                    "ubuntu": 1
                }
            }
        }, {
            "name": "machine_email",
            "enabled": false,
            "config": {
                "datacenter": "DATACENTER_NAME",
                "smtp": {
                    "host": "127.0.0.1",
                    "port": 25,
                    "use_authentication": false,
                    "user": "",
                    "pass": ""
                },
                "from": "nobody@joyent.com",
                "subject": "Your SmartDataCenter machine is provisioning",
                "body": "Check /my/machines for updates"
            }
        }
    ],
    "userThrottles": {
        "all": {
            "username": true,
            "burst": 30,
            "rate": 10,
            "overrides": {
                "SDC_ADMIN_LOGIN": {
                    "burst": 0,
                    "rate": 0
                }
            }
        },
        "analytics": false
    }
}
